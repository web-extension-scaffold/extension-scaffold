{
    "name": "@moesol/ext-example-vite",
    "version": "2.12.0-SNAPSHOT",
    "dependencies": {
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "@vitejs/plugin-react": "~4.2.1",
        "dotenv": "~16.4.5"
    },
    "scripts": {
        "start": "vite dev",
        "build": "vite build && mkdir -p dist/dist && cp -rf dist/*.js dist/dist/",
        "check-types": "tsc --noEmit",
        "maybe-install": "[ -d node_modules ] && [ -z $DO_INSTALL ] || npm install",
        "container": "npm run maybe-install && vite dev",
        "container-preview": "npm run maybe-install && npm run build && vite preview --outDir ."
    },
    "devDependencies": {
        "@moesol/es-runtime": "workspace:*",
        "@rollup/plugin-commonjs": "^18.0.0",
        "@rollup/plugin-node-resolve": "^11.2.1",
        "@rollup/plugin-replace": "^2.4.2",
        "@types/react": "~17.0.3",
        "@types/react-dom": "~17.0.3",
        "rollup": "^2.45.2",
        "rollup-plugin-serve": "^1.1.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "tslib": "^2.2.0",
        "typescript": "~4.8.2",
        "vite": "~5.2.9",
        "vite-plugin-node-polyfills": "~0.22.0"
    }
}
